
<%= form_for @profile, url: {action: "create"} do |f| %>
  <%= f.label :employee_number, "Employee Number" %>
  <%= f.text_field :employee_number %><br />

  <%= f.label :ssn, "SSN (last 4 digits)"  %>
  <%= f.text_field :ssn %><br />

  <%= f.button "Submit", class: "btn btn-default btn-success" %>
<% end %>
